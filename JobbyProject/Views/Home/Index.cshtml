    <!-- OUR ROOMS-->

    <section class="section bg-blue p-t-120 p-b-165">
        <div class="bg-cloud"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="section-heading section-heading-2">
                        <h2 class="section-heading__title">наши комнаты & классы</h2>
                        <div class="section-heading__line">
                            <img src="~/Public/images/icon/line-white.png" alt="Line 2">
                        </div>
                    </div>
                </div>
            </div>
            <div class="row no-gutters">
                @foreach (var item in Model.classes)
                {
                    <div class="col-lg-6">
                        <div class="p-r-25 p-md-r-0">
                            <div class="media media-our-class">
                                <div class="media__body">
                                    <h3 class="media__title">
                                        <a href="@Url.Action("index","classes",new {id=item.ID })">@item.Name</a>
                                    </h3>
                                    <span class="date-time">@item.OpenHours.ToString("hh:mm") - @item.CloseHours.ToString("HH:mm") </span>
                                    <span class="date-time">@item.StartDate.ToString("dd MMMM yyyy")</span>
                                    <div class="media-our-class__info">
                                        <div class="row no-gutters">
                                            <div class="col-6">
                                                <div class="item has-seprator">
                                                    <span class="name">Возраст</span>
                                                    <span class="value">@item.age.MinAge - @item.age.MaxAge лет</span>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="item">
                                                    <span class="name">Мест</span>
                                                    <span class="value">@item.SizeSeat детей</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="media__img">
                                    <a href="@Url.Action("index","classes")">
                                        <img src="~/Public/images//@item.Image" alt="Our room 1">
                                    </a>
                                    <span class="media-our-class__price">
                                        <span class="inner">@item.Price АЗН</span>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>

                }

              
                    <div class="row">
                        <div class="col-md-12">
                            <div class="learn-more text-center m-t-60">
                                <a class="au-btn au-btn--pink" href="@Url.Action("index","classes")">
                                    Узнать больше
                                    <i class="zmdi zmdi-chevron-right"></i>
                                    <i class="zmdi zmdi-chevron-right"></i>
                                </a>
                            </div>
                        </div>
                    </div>

                </div>
        </div>
    </section>
    <!-- END OUR ROOMS-->
